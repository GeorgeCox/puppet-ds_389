{
  "description": "Allows you to reinitialize replication for a consumer",
  "input_method": "stdin",
  "parameters": {
    "instance_name": {
      "description": "Directory Server instance name",
      "type": "String[1]"
    },
    "replica_name": {
      "description": "Replica name to initialize",
      "type": "String[1]"
    },
    "suffix": {
      "description": "LDAP suffix to use",
      "type": "String[1]"
    },
    "server_host": {
      "description": "Host to connect to",
      "type": "String[1]"
    },
    "server_port": {
      "description": "SSL port to connect to",
      "type": "Integer"
    },
    "protocol": {
      "description": "Connection protocol",
      "type": "String[1]"
    },
    "starttls": {
      "description": "Connect with StartTLS",
      "type": "Boolean"
    },
    "root_dn": {
      "description": "The root dn to bind as",
      "type": "String[1]"
    },
    "root_dn_pass": {
      "description": "The root dn password",
      "type": "String[1]"
    }
  }
}
